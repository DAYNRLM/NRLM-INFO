<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_nav_graph"
    app:startDestination="@id/homeFragment2">
    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.nrlminfo.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment2_to_aboutNrlmFragment"
            app:destination="@id/aboutNrlmFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_referFragment2"
            app:destination="@id/referFragment2" />
        <action
            android:id="@+id/action_homeFragment2_to_backgroundFragment"
            app:destination="@id/backgroundFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_objectiveFragment"
            app:destination="@id/objectiveFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_FAQ_Fragment"
            app:destination="@id/FAQ_Fragment" />
        <action
            android:id="@+id/action_homeFragment2_to_activityFragment"
            app:destination="@id/activityFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_contactsFragment"
            app:destination="@id/contactsFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_feedbackFragment"
            app:destination="@id/feedbackFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.nrlminfo.ui.home.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_changeLanguageFragment"
            app:destination="@id/changeLanguageFragment" />
        <action
            android:id="@+id/action_profileFragment_to_aboutUsFragment"
            app:destination="@id/aboutUsFragment" />
    </fragment>
    <action
        android:id="@+id/action_global_profileFragment"
        app:destination="@id/profileFragment"
        app:popUpTo="@id/homeFragment2" />
    <fragment
        android:id="@+id/aboutNrlmFragment"
        android:name="com.example.nrlminfo.ui.home.AboutNrlmFragment"
        android:label="About NRLM"
        tools:layout="@layout/fragment_about_nrlm" />
    <fragment
        android:id="@+id/referFragment2"
        android:name="com.example.nrlminfo.ui.home.ReferFragment"
        android:label="fragment_refer"
        tools:layout="@layout/fragment_refer" />
    <fragment
        android:id="@+id/backgroundFragment"
        android:name="com.example.nrlminfo.ui.home.BackgroundFragment"
        android:label="Background"
        tools:layout="@layout/fragment_background" />
    <fragment
        android:id="@+id/changeLanguageFragment"
        android:name="com.example.nrlminfo.ui.home.ChangeLanguageFragment"
        android:label="fragment_change_language"
        tools:layout="@layout/fragment_change_language" />
    <fragment
        android:id="@+id/aboutUsFragment"
        android:name="com.example.nrlminfo.ui.home.AboutUsFragment"
        android:label="fragment_aout_us"
        tools:layout="@layout/fragment_aout_us" />
    <fragment
        android:id="@+id/contactsFragment"
        android:name="com.example.nrlminfo.ui.home.contacts.ContactsFragment"
        android:label="Contacts"
        tools:layout="@layout/fragment_contacts" />
    <fragment
        android:id="@+id/feedbackFragment"
        android:name="com.example.nrlminfo.ui.home.feedback.FeedbackFragment"
        android:label="Feedback"
        tools:layout="@layout/fragment_feedback" />
    <fragment
        android:id="@+id/objectiveFragment"
        android:name="com.example.nrlminfo.ui.home.ObjectiveFragment"
        android:label="Objective"
        tools:layout="@layout/fragment_objective" />
    <fragment
        android:id="@+id/FAQ_Fragment"
        android:name="com.example.nrlminfo.ui.home.FAQ_Fragment"
        android:label="fragment_faq"
        tools:layout="@layout/fragment_faq" />
    <fragment
        android:id="@+id/activityFragment"
        android:name="com.example.nrlminfo.ui.home.ActivityFragment"
        android:label="Components/Activities"
        tools:layout="@layout/fragment_component_activity" />
    <dialog
        android:id="@+id/logoutFragment2"
        android:name="com.example.nrlminfo.dialog.LogoutFragment"
        android:label="dialog_layout_logout"
        tools:layout="@layout/dialog_layout_logout" />
</navigation>